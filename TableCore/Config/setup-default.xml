    <patterns>
      <int>^(\-|\+)?\d+$</int>
      <positive_int>^[1-9]\d*$</positive_int>
      <uint>^\+?\d+$</uint>
      <float>^(\-|\+)?\d+(\.\d+)?$</float>
      <string>[\w\W]*</string>
      <enum>^([a-zA-Z_]+[0-9]*|\d+)$</enum>
      <mask>^([z-zA-Z_,])*$</mask>
      <int_array>^\[((\-|\+)?\d+,?)*\]$</int_array>
      <matrix>^\[(\[((\-|\+)?\d+,?)*\],?)*\]$</matrix>
      <time>^(20|21|22|23|[0-1]\d):[0-5]\d(:[0-5]\d)?$</time>
      <vector>^\(((\-|\+)?\d+(\.\d+)?,?)*\)$</vector>
      <date>^\d+/\d+/\d+$</date>
      <prop>^(((@[\d\w\W]+ *)+|[\w\W\d]+) *(\* *[\d]+)?)?$</prop>
      <prop_array>^\[(((@[\d\w\W]+ *)+|[\w\W\d]+) *(\* *[\d]+)?,?)*\]$</prop_array>
      <range>^(((in)|(out))(\(|\[)[\w0-9]+,[\w0-9]+(\)|\]))|([\d]+)$</range>
      <ftable>^\[(,?[\w\W]+ *= *(\-|\+)?\d+(\.\d+)?)*\]$</ftable>
      <task_array>^\[((,)?(none|collect|talk|action|build|human|animal|equipment|giveanimal)_[\d]+)*\]$</task_array>
      <duration_time>^(\d+):[0-5]\d(:[0-5]\d)?$</duration_time>
      <animal>^(((@[\d\w\W]+ *)+|[\w\W\d]+) *(\* *[\d]+)?)?$</animal>
      <reward_type>^\[((,)?(feeling|money|prop)_[\d]+)*\]$</reward_type>
    </patterns>

